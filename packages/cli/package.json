{
  "name": "oonkoo",
  "version": "1.0.0",
  "description": "CLI for installing OonkooUI components",
  "type": "module",
  "bin": {
    "oonkoo": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup src/index.ts --format esm --watch",
    "build": "tsup src/index.ts --format esm --dts",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "oonkoo",
    "oonkooui",
    "components",
    "ui",
    "cli",
    "react",
    "tailwind"
  ],
  "author": "OonkooUI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oonkoo/oonkoo-ui"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "conf": "^13.0.1",
    "execa": "^9.5.2",
    "fs-extra": "^11.2.0",
    "open": "^10.1.0",
    "ora": "^8.1.1",
    "prompts": "^2.4.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.17.10",
    "@types/prompts": "^2.4.9",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18"
  }
}
